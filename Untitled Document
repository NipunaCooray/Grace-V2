CREATE TABLE style(
ID INTEGER PRIMARY KEY AUTOINCREMENT,
styleNo		TEXT,
size		TEXT,
side		TEXT,
color		TEXT,
tension_1	INTEGER,
tension_2	INTEGER,
tension_3	INTEGER,
tension_4	INTEGER,
speed_1		INTEGER,
speed_2		INTEGER,
speed_3		INTEGER,
speed_4		INTEGER,
ms_1		INTEGER,
ms_2		INTEGER,
ms_3		INTEGER,
ms_4		INTEGER,
prt_1		INTEGER,
prt_2		INTEGER,
prt_3		INTEGER,
prt_4		INTEGER,
prb_1		INTEGER,
prb_2		INTEGER,
prb_3		INTEGER,
prb_4		INTEGER,
puller_1	INTEGER,
puller_2	INTEGER,
puller_3	INTEGER,
puller_4	INTEGER,
pti		INTEGER,
pto		INTEGER,
pbi		INTEGER,
pbo		INTEGER,
si		INTEGER,
so		INTEGER,
d1		INTEGER,
d2		INTEGER,
d3		INTEGER,
d4		INTEGER,
d5		INTEGER,
d6		INTEGER,
d7		INTEGER,
d8		INTEGER,
feeler_thresh	INTEGER,
num_regions	INTEGER
);
